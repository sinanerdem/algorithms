#include "mult150.h"

mult150::mult150(char* a, char* b): _m1(NULL), _m2(NULL){
	_lena = strlen(a);
	_m1 = new int[_lena];
	for(int i=0;i<=_lena;i++){
		_m1[i] = a[i]-'0';
	}
	_lenb = strlen(b);
	_m2 = new int[_lenb];
	for(int i=0;i<=_lenb;i++){
		_m2[i] = b[i]-'0';
	}
}
mult150::~mult150(){
	delete[] _m1;
	delete[] _m2;
}
void mult150::_multiply(){
	for(int i=_lenb ; i>=0 ; i--){

	}
}
